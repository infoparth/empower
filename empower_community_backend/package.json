{
  "name": "dao-template",
  "devDependencies": {
    "@nomicfoundation/hardhat-verify": "^1.1.1",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@0.3.0-beta.12",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "@openzeppelin/contracts": "^4.9.3",
    "@typechain/ethers-v5": "^9.0.0",
    "@typechain/hardhat": "^4.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.13",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.3",
    "fs": "^0.0.1-security",
    "fs-extra": "^10.0.0",
    "hardhat": "^2.8.3",
    "hardhat-deploy": "^0.10.4",
    "hardhat-gas-reporter": "^1.0.7",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.18",
    "ts-node": "^10.4.0",
    "typechain": "^7.0.0",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "lint": "solhint 'contracts/*.sol'",
    "lint:fix": "solhint 'contracts/**/*.sol' --fix",
    "format": "yarn prettier --write .",
    "typechain": "yarn hardhat typechain",
    "ci:clean": "rm -rf ./artifacts ./cache .;types",
    "deploy-testnet": "cross-env NODE_ENV=dev npx hardhat deploy --network polygonAmoy",
    "deploy-mainnet": "cross-env NODE_ENV=prod npx hardhat deploy --network polygon_mainnet",
    "vote": "cross-env NODE_ENV=dev npx hardhat run scripts/vote.ts --network polygonAmoy",
    "change-owner": "cross-env NODE_ENV=dev npx hardhat run scripts/changeOwnerToken.ts --network polygonAmoy",
    "check-value": "cross-env NODE_ENV=dev npx hardhat run scripts/checkValue.ts --network polygonAmoy",
    "queue":"cross-env NODE_ENV=dev npx hardhat run scripts/queue-and-execute.ts --network polygonAmoy",
    "compile": "cross-env NODE_ENV=prod npx hardhat compile",
    "test": "cross-env NODE_ENV=prod npx hardhat test"
  },
  "dependencies": {
    "erc721a": "^4.2.3"
  }
}
